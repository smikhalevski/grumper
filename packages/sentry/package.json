{
  "name": "@pipit/sentry",
  "version": "1.0.0",
  "description": "Pipit message processor for Sentry.",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
