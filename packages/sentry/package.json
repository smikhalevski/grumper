    "sentry"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/pipit/issues"
  },
  "homepage": "https://github.com/smikhalevski/pipit/tree/master/packages/sentry#readme",
  "peerDependencies": {
    "pipit": "^1.0.0"
  },
  "devDependencies": {
    "@sentry/types": "^7.21.1"
  }
